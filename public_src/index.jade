html(lang='en', ng-app='tvAuction')
head
    meta(charset='utf-8')
    title Tv Auction
    link(rel='stylesheet', href='lib/bootstrap/css/bootstrap.css')
    link(rel='stylesheet', href='css/app.css')
  body
    .row
      .span9.center
        .navbar
          .navbar-inner
            .container
              a.brand(ng-href='#/') TvAuction
              ul.nav
                li: a(ng-href='#/campaign') List Campaigns
                li: a(ng-href='#/auction') List Auctions
                li: a(ng-href='#/help') Help
                li: a(ng-href='#/user/login') Log In
                li: a(ng-href='#/user/logout') Log Out
        div(ng-view)

    script(src='lib/jquery/jquery-1.7.2.js')
    script(src='lib/underscore/underscore-1.3.3.js')
    script(src='lib/bootstrap/bootstrap-2.1.1.js')
    script(src='lib/d3/d3.v2.js')
    script(src='lib/angular/angular.js')
    script(src='lib/angular-ui-bootstrap/ui-bootstrap-tpls-0.1.0.js')
    script(src='js/app.js')
    script(src='js/services.js')
    script(src='js/controllers.js')
    script(src='js/filters.js')
    script(src='js/directives.js')
    script(src='js/graphs.js')
